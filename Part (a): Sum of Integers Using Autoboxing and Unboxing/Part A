import java.util.*;

public class PartA_SumAutoboxing {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        List<Integer> numbers = new ArrayList<>();

        System.out.println("Enter integers separated by space:");
        String input = sc.nextLine();

        // Split and parse using Integer.parseInt() (Auto-boxing happens here)
        String[] parts = input.split(" ");
        for (String part : parts) {
            if (!part.isEmpty()) {
                int num = Integer.parseInt(part); // parsing string to primitive int
                numbers.add(num); // autoboxing (int → Integer)
            }
        }

        // Calculate sum using unboxing
        int sum = 0;
        for (Integer n : numbers) { // unboxing (Integer → int)
            sum += n;
        }

        System.out.println("Sum of integers: " + sum);
        sc.close();
    }
}
